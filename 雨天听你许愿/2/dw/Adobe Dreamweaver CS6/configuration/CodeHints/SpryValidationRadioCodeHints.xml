<codehints xmlns:MMString="http://www.adobe.com/schemes/data/string/">
<menugroup MMString:name="codehints_spry" id="CodeHints_Spry">
  <description>
  <MMString:loadString id="codehints_spry" />
  </description>
  
  <menu pattern="Spry.Widget." casesensitive="true">
    <menuitem label="ValidationRadio" icon="shared/mm/images/hintClass.png" />
  </menu>  

   <function pattern="Spry.Widget.ValidationRadio(radioWidgetID, {options})" caseSensitive="true" displayrestriction="JavaScript">
    <parammenu pattern='{,' name="options" type="optionArray" allowwhitespaceprefix="true" index="1">
      <optionparammenu pattern="isRequired" label="isRequired" value="isRequired:" datatype="enumerated" icon="shared/mm/images/hintMisc.gif">
        <optionparammenuitem label="true" value="true" icon="shared/mm/images/hintMisc.gif"/>
        <optionparammenuitem label="false" value="false" icon="shared/mm/images/hintMisc.gif"/>
      </optionparammenu>
      <optionparammenu pattern="emptyValue" label="emptyValue" value="emptyValue:" datatype="string" icon="shared/mm/images/hintMisc.gif"></optionparammenu>
      <optionparammenu pattern="invalidValue" label="invalidValue" value="invalidValue:" datatype="string" icon="shared/mm/images/hintMisc.gif"></optionparammenu>
      <optionparammenu pattern="focusClass" label="focusClass" value="focusClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
      <optionparammenu pattern="invalidClass" label="invalidClass" value="invalidClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
      <optionparammenu pattern="requiredClass" label="requiredClass" value="requiredClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
      <optionparammenu pattern="validClass" label="validClass" value="validClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
      
    </parammenu>
  </function>

  <menu classpattern="Spry.Widget.ValidationRadio" displayrestriction="JavaScript">
    <method pattern="validate()" icon="shared/mm/images/hintFunction.png" />
    <method pattern="reset()" icon="shared/mm/images/hintFunction.png" />
  </menu>
</menugroup>
</codehints>