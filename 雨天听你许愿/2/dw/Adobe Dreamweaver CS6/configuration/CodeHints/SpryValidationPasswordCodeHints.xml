<codehints xmlns:MMString="http://www.adobe.com/schemes/data/string/">
<menugroup MMString:name="codehints_spry" id="CodeHints_Spry">
  <description>
  <MMString:loadString id="codehints_spry" />
  </description>
  
  <menu pattern="Spry.Widget." casesensitive="true">
    <menuitem label="ValidationPassword" icon="shared/mm/images/hintClass.png" />
  </menu>	
  
  <function pattern="Spry.Widget.ValidationPassword(element, {options})" caseSensitive="true" displayrestriction="JavaScript">
    <parammenu pattern='{,' name="options" type="optionArray" allowwhitespaceprefix="true" index="1">
      <parammenuitem label="additionalError" value="additionalError:" type="string" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="maxChars" value="maxChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="minChars" value="minChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="maxAlphaChars" value="maxAlphaChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="minAlphaChars" value="minAlphaChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="maxUpperAlphaChars" value="maxUpperAlphaChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="minUpperAlphaChars" value="minUpperAlphaChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="maxNumbers" value="maxNumbers:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="minNumbers" value="minNumbers:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="maxSpecialChars" value="maxSpecialChars:" icon="shared/mm/images/hintMisc.gif"/>
      <parammenuitem label="minSpecialChars" value="minSpecialChars:" icon="shared/mm/images/hintMisc.gif"/>
      <optionparammenu pattern="isRequired" label="isRequired" value="isRequired:" type="enumerated" icon="shared/mm/images/hintMisc.gif">
         <optionparammenuitem label="true" value="true" icon="shared/mm/images/hintMisc.gif"/>
         <optionparammenuitem label="false" value="false" icon="shared/mm/images/hintMisc.gif"/>
      </optionparammenu>
      <optionparammenu pattern="validateOn" label="validateOn" value="validateOn:" icon="shared/mm/images/hintFlag.gif" type="enumerated">
        <optionparammenuitem label="blur" value="blur"	icon="shared/mm/images/hintMisc.gif"/>
        <optionparammenuitem label="change" value="change"	icon="shared/mm/images/hintMisc.gif"/>
      </optionparammenu>
      <optionparammenu pattern="invalidClass" label="invalidClass" value="invalidClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
      <optionparammenu pattern="requiredClass" label="requiredClass" value="requiredClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
      <optionparammenu pattern="validClass" label="validClass" value="validClass:" type="cssStyle" datatype="string" icon="shared/mm/images/hintCss.gif"></optionparammenu>
    </parammenu>
	</function>
    

  <menu classpattern="Spry.Widget.ValidationPassword" displayrestriction="JavaScript">
    <method pattern="validate()" icon="shared/mm/images/hintFunction.png" />
    <method pattern="reset()" icon="shared/mm/images/hintFunction.png" />
  </menu>
</menugroup>
</codehints>


